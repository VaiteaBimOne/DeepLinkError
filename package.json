{
  "name": "bimtrack-mobile",
  "version": "1.5.4",
  "private": true,
  "scripts": {
    "check-dependencies": "rnx-dep-check",
    "fix-dependencies": "rnx-dep-check --write",
    "postinstall": "husky install && patch-package",
    "android": "react-native run-android",
    "android:release": "react-native run-android --variant=release",
    "android:internal": "react-native run-android --variant=internal",
    "reconnect": "adb reverse tcp:8081 tcp:8081",
    "ios": "react-native run-ios",
    "ios:release": "react-native run-ios --configuration Release",
    "ios:internal": "react-native run-ios --configuration Internal",
    "start": "react-native start",
    "test": "jest --maxConcurency=4 --maxWorker=4 --watchAll=false --coverage",
    "test:ci": "jest --maxConcurency=4 --maxWorker=4 --watchAll=false --filter=./testFilter.js  --coverage",
    "test:tdd": "jest --detectOpenHandles --watchAll=true --notify",
    "lint": "yarn lint:style && yarn lint:code",
    "lint:fix": "yarn fix:code && yarn fix:style",
    "lint:code": "eslint src/ --ext .js,.jsx,.ts,.tsx --quiet",
    "lint:style": "prettier --check \"src/**/**.{js,ts,jsx,tsx}\"",
    "fix:style": "prettier -w \"src/**/**.{js,ts,jsx,tsx}\"",
    "fix:code": "eslint src/ --ext .js,.jsx,.ts,.tsx --fix --quiet",
    "pod": "cd ios && pod install",
    "pod:update": "cd ios && pod update",
    "nuke": "rm -rf ~/Library/Caches/CocoaPods && (cd ios && rm -rf Pods && pod install)",
    "restart": "yarn clean-all && yarn start --reset-cache",
    "tsc": "tsc",
    "bundle:release:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/build/intermediates/res/merged/release/ && rm -rf android/app/src/main/res/drawable-* && rm -rf android/app/src/main/res/raw/* && cd android && ./gradlew assembleRelease && cd ..",
    "bundle:debug:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/ && cd android && ./gradlew assembleDebug && cd ..",
    "release": "react-native-version && standard-version"
  },
